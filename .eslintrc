{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "airbnb/legacy",
  "parser": "babel-eslint",
  "plugins": ["compat"],
  "rules": {
    "compat/compat": "warn",

    "no-plusplus": "off",
    "no-param-reassign": "off",
    "no-use-before-define": ["error", { "functions": false, "classes": false }]
  }
}
